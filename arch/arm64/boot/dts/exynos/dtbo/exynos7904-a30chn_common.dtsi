&spi_0{
	status = "okay";
	/delete-property/ pinctrl-names;
	/delete-property/ pinctrl-0;

	ese_spi@0 {
		compatible = "ese_p3";
		reg = <0>;
		spi-max-frequency = <13000000>;
		gpio-controller;
		#gpio-cells = <2>;

		p3-vdd_1p8-gpio = <&gpf3 3 1>;
		ese_p3,cs-gpio = <&gpp5 1 0>;

		clocks = <&clock GATE_SPI0>, <&clock SPI_0>;
		clock-names = "pclk", "sclk";

		pinctrl-names = "ese_active", "ese_suspend";
		pinctrl-0 = <&spi0_bus_ese &spi0_ese_cs_gpio>;
		pinctrl-1 = <&spi0_bus_ese_suspend>;

		controller-data {
			samsung,spi-feedback-delay = <0>;
			samsung,spi-chip-select-mode = <0>;
		};
	};
};

&i2c_5 {
  	        status = "okay";

 		#address-cells = <1>;
 		#size-cells = <0>;
 		samsung,i2c-sda-delay = <100>;
 		samsung,i2c-max-bus-freq = <400000>;
 		pinctrl-names = "default";
 		pinctrl-0 = <&i2c5_bus>;

 		stk@47 {
			compatible = "stk,stk3x3x";
			reg = <0x47>;
			pinctrl-names = "default";
			pinctrl-0 = <&proxy_int>;
			interrupt-parent = <&gpa0>;
			interrupts = <4 0 0>;
			stk,gpio_int = <&gpa0 4 0>;
			stk,prox_thd_h = <150>;
			stk,prox_thd_l = <140>;
			stk,sunlight_thd_h = <240>;
			stk,sunlight_thd_l = <220>;
			stk,first_cal_adc_limit = <200>;
			stk,first_cal_thd_h = <240>;
			stk,first_cal_thd_l = <220>;
			stk,intel_prst = <0>;
			stk,ps_it = <0x04>;
		};
};

