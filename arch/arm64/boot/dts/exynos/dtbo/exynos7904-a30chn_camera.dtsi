#include <dt-bindings/camera/fimc_is.h>
/ {
	fragment@camera {
		target-path = "/";
		__overlay__ {
			/* REAR CAMERA */
			fimc_is_module_3l6: fimc-is_sensor_3l6@2D {
				compatible = "samsung,sensor-module-3l6";

				use_pdaf; /* when USE_AP_PDAF is enabled, use for pdaf sensor */

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&fimc_is_mclk2_out>;
				pinctrl-2 = <&fimc_is_mclk2_fn>;
				pinctrl-3 = <>;

				position = <0>;					/* Rear:0. Front:1 */
				id = <0>; 						/* bns_id */
				mclk_ch = <2>;					/* XCI0_MCLK */
				sensor_i2c_ch = <1>;				/* HSI2C1 */

				gpio_mclk = <&gpc0 2 0x1>;			/* RCAM1_RCAM2_MCLK - XCI0_MCLK */
				gpio_reset = <&gpg3 4 0x1>;			/* RCAM1_RST_N - XGPIO24 */
				gpio_camaf_2p8_en = <&gpg1 5 0x1>;	/* RCAM1_AFVDD_2P8_EN*/
				shared_mclk;
				status = "okay";
				af {
					product_name = <19>;			/* ACTUATOR_NAME_DW9808 */
					i2c_addr = <0x18>;
					i2c_ch = <3>;
				};
				flash {
					product_name = <11>; /* FLASH_GPIO */
					flash_first_gpio = <2>;
				};
			};
			/* FRONT CAMERA */
			fimc_is_sensor_3p8sp: fimc-is_sensor_3p8sp@10 {
				compatible = "samsung,sensor-module-3p8sp";
				pinctrl-names = "default", "pin1", "pin2", "release";
				pinctrl-0 = <&fimc_is_mclk1_out>;
				pinctrl-1 = <&fimc_is_mclk1_out>;
				pinctrl-2 = <&fimc_is_mclk1_fn>;
				pinctrl-3 = <>;

				position = <1>;				/* Rear:0. Front:1 .. Rear3:6*/
				id = <1>;					/* bns_id */
				mclk_ch = <1>;
				sensor_i2c_ch = <0>;
				num_of_lanes=<4>;			/*  LANES_1:1, LANES_2:2, LANES_3:3, LANES_4:4 */

				gpio_mclk = <&gpc0 1 0x1>;				/* FCAM_MCLK*/
				gpio_reset = <&gpg3 5 0x1>;				/* FCAM_RST_N */
				status = "okay";
			};
			/* ULTRA WIDE CAMERA */
			fimc_is_module_5e9: fimc-is_sensor_5e9@10 {
				compatible = "samsung,sensor-module-5e9";

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <&fimc_is_mclk0_out>;
				pinctrl-1 = <&fimc_is_mclk0_out>;
				pinctrl-2 = <&fimc_is_mclk0_fn>;
				pinctrl-3 = <>;

				position = <2>;							/* Rear:0. Front:1 */
				id = <2>;									/* bns_id */
				mclk_ch = <0>;
				sensor_i2c_ch = <2>;						/* HSI2C3 */

				gpio_mclk = <&gpc0 0 0x1>;					/* RCAM1_RCAM2_MCLK - XCI0_MCLK */
				gpio_reset = <&gpg2 5 0x1>;					/* RCAM2_RST_N - XGPIO17 */
				status = "okay";
			};
			/* DEPTH CAMERA */
			fimc_is_module_gc5035: fimc-is_sensor_gc5035@37 {
				compatible = "samsung,sensor-module-gc5035";

				pinctrl-names = "pin0", "pin1", "pin2", "release";
				pinctrl-0 = <>;
				pinctrl-1 = <&fimc_is_mclk2_out>;
				pinctrl-2 = <&fimc_is_mclk2_fn>;
				pinctrl-3 = <>;

				position = <4>;							/* Rear:0. Front:1 */
				id = <3>;									/* bns_id */
				mclk_ch = <2>;
				sensor_i2c_ch = <2>;						/* HSI2C3 */

				gpio_mclk = <&gpc0 2 0x1>;					/* RCAM1_RCAM2_MCLK - XCI0_MCLK */
				gpio_reset = <&gpg1 7 0x1>;					/* RCAM2_RST_N - XGPIO17 */
				gpio_1p2_a2p8_en = <&gpg1 3 0x1>; 				/* CAM_DVDD_1P2 CAM_AVDD_2P8 */
				shared_mclk;
				status = "okay";
			};

			fimc_is_flash_gpio: fimc-is-flash_gpio@0 {
				compatible = "samsung,sensor-flash-gpio";
				id = <0>; /* Rear  */
				status = "okay";
				flash-gpio = <&gpg2 7 0>;
				torch-gpio = <&gpg1 0 0>;

			};
		}; /* end of __overlay__ */
	}; /* end of fragment */
}; /* end of root */
